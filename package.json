{
    "name": "pow.js",
    "version": "0.1.0-alpha.0",
    "description": "Run spec, benchmark and impact tests, all from one tool",
    "keywords": [
        "framework",
        "test",
        "spec",
        "benchmark",
        "impact"
    ],
    "repository": "futagoza/pow",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "bin",
        "dist",
        "lib"
    ],
    "main": "index.js",
    "bin": {
        "pow": "bin/pow",
        "impact": "bin/impact"
    },
    "browser": "dist/pow.js",
    "scripts": {
        "lint": "eslint .",
        "test": "nyc mocha",
        "postversion": "git push --follow-tags && npm publish",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
    },
    "dependencies": {
        "chai": "4.2.0",
        "bluebird": "3.5.3",
        "lodash": "4.17.11"
    },
    "devDependencies": {
        "codecov": "3.2.0",
        "eslint": "5.14.0",
        "eslint-config-futagozaryuu": "5.6.1",
        "mocha": "5.2.0",
        "nyc": "13.3.0"
    },
    "engines": {
        "node": ">= 4"
    }
}
