{
    "name": "pow.js",
    "version": "0.1.0-alpha.0",
    "description": "Run spec, benchmark and impact tests, all from one tool",
    "keywords": [
        "framework",
        "test",
        "spec",
        "benchmark",
        "impact"
    ],
    "repository": "futagoza/pow",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "bin",
        "dist",
        "lib"
    ],
    "main": "index.js",
    "bin": {
        "pow": "bin/pow",
        "impact": "bin/impact"
    },
    "browser": "dist/pow.js",
    "scripts": {
        "dist:build": "webpack",
        "dist:watch": "webpack --watch",
        "docs:build": "vuepress build docs",
        "docs:watch": "vuepress dev docs",
        "lint": "eslint .",
        "test": "nyc mocha",
        "postversion": "git push --follow-tags && npm publish",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
    },
    "dependencies": {
        "chai": "4.2.0",
        "bluebird": "3.5.3",
        "lodash": "4.17.11"
    },
    "devDependencies": {
        "@babel/core": "7.3.3",
        "@babel/preset-env": "7.3.1",
        "babel-loader": "8.0.5",
        "codecov": "3.2.0",
        "dedent": "0.7.0",
        "eslint": "5.14.0",
        "eslint-config-futagozaryuu": "5.6.1",
        "eslint-plugin-vue": "5.2.1",
        "glob": "7.1.3",
        "mocha": "5.2.0",
        "nyc": "13.3.0",
        "webpack": "4.29.5",
        "webpack-cli": "3.2.3",
        "vuepress": "1.0.0-alpha.39"
    },
    "engines": {
        "node": ">= 4"
    }
}
