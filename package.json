{
    "name": "pow.js",
    "version": "0.1.0-alpha",
    "description": "Run spec, benchmark and impact tests, all from one tool",
    "keywords": [
        "framework",
        "test",
        "spec",
        "benchmark",
        "impact"
    ],
    "repository": "futagoza/pow",
    "bugs": "https://github.com/futagoza/pow/issues",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "bin",
        "dist",
        "lib"
    ],
    "main": "lib/nodejs/index.js",
    "bin": {
        "pow": "bin/pow.js",
        "impact": "bin/impact.js"
    },
    "browser": "dist/pow.browser.js",
    "typings": "index.d.ts",
    "scripts": {
        "compile": "run-p generate:* bundle:*",
        "generate:core": "babel src/core -d lib/core",
        "generate:nodejs": "babel src/nodejs -d lib/nodejs",
        "generate:extensions": "babel src/extensions -d lib/extensions",
        "bundle:core": "ROLLUP_TARGET=core rollup -c",
        "bundle:browser": "ROLLUP_TARGET=browser rollup -c",
        "bundle:extensions": "ROLLUP_TARGET=extensions rollup -c",
        "lint": "eslint --cache bin src rollup.config.js",
        "test": "jest",
        "preversion": "npm install && jest --silent",
        "postversion": "git push --follow-tags && npm publish"
    },
    "dependencies": {
        "babel-polyfill": "7.0.0-alpha.20",
        "babel-runtime": "7.0.0-alpha.20"
    },
    "devDependencies": {
        "npm-run-all": "4.1.x",
        "eslint": "4.6.x",
        "eslint-config-futagozaryuu": "3.5.x",
        "jest": "21.0.x",
        "@types/jest": "20.0.x",
        "babel-register": "7.0.0-alpha.20",
        "babel-cli": "7.0.0-alpha.20",
        "babel-preset-futagozaryuu": "2.8.x",
        "rollup": "0.49.x",
        "rollup-plugin-babel": "3.0.x",
        "rollup-plugin-json": "2.3.x",
        "uglify-js": "3.0.x"
    },
    "engines": {
        "node": ">= 4"
    },
    "eslintConfig": {
        "extends": "futagozaryuu/node-v4",
        "root": true
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "testMatch": [
            "**/*.(spec|test).js"
        ]
    }
}
