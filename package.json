{
    "publishConfig": {
        "access": "public"
    },
    "name": "@futagoza/pow",
    "version": "0.1.0-alpha.0",
    "description": "Run spec, benchmark and impact tests, all from one tool",
    "keywords": [
        "framework",
        "test",
        "spec",
        "benchmark",
        "impact"
    ],
    "repository": "futagoza/pow",
    "license": "MIT",
    "author": "Futago-za Ryuu <futagoza.ryuu@gmail.com>",
    "files": [
        "bin",
        "dist",
        "lib"
    ],
    "main": "index.js",
    "bin": {
        "pow": "bin/pow",
        "impact": "bin/impact"
    },
    "browser": "dist/pow.js",
    "scripts": {
        "lint": "eslint .",
        "spec": "mocha --reporter progress",
        "clean": "rimraf .nyc_output coverage dist",
        "dist:build": "webpack",
        "dist:watch": "webpack --watch",
        "test": "nyc mocha",
        "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
        "prepack": "npm run dist:build",
        "preversion": "npm run clean",
        "postversion": "git push --follow-tags && npm publish"
    },
    "dependencies": {
        "@sinonjs/referee-sinon": "5.0.0",
        "bluebird": "3.7.0",
        "cosmiconfig": "5.2.1",
        "chai": "4.2.0",
        "emittery": "0.4.1",
        "glob": "7.1.4",
        "mkdirp": "0.5.1",
        "lodash": "4.17.15"
    },
    "devDependencies": {
        "@babel/core": "7.6.2",
        "@babel/preset-env": "7.6.2",
        "@futagoza/eslint-config": "10.0.0",
        "@types/node": "8",
        "babel-loader": "8.0.6",
        "codecov": "3.6.1",
        "dedent": "0.7.0",
        "eslint": "6.5.1",
        "mocha": "6.2.1",
        "nyc": "14.1.1",
        "rimraf": "3.0.0",
        "webpack": "4.41.0",
        "webpack-cli": "3.3.9"
    },
    "engines": {
        "node": ">= 8"
    }
}
